<?xml version="1.0" encoding="utf-8"?>
<Defs>
   <ThingDef Name="NekosocksBase" Abstract="True" ParentName="NekoApparelBase">
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
      <displayPriority>205</displayPriority>
    </recipeMaker>
    <costList> 
      <Cloth>30</Cloth>
    </costList>
    <thingCategories>
      <li>ApparelMisc</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>1600</WorkToMake>
      <Mass>0.25</Mass>
      <StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.22</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.10</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>1.5</EquipDelay>
    </statBases>
    <apparel>
    <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
      <li>Legs</li>
      </bodyPartGroups>
      <layers>
        <li>Underwear</li>
      </layers>
      <tags>
        <li>nekobasic</li>
      </tags>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
    </apparel>
    <tradeTags>
      <li>BasicClothing</li>
    </tradeTags>
   </ThingDef>
    


